NoOp {
 name NoOp1
 label variables
 selected true
 xpos 994
 ypos 14
 addUserKnob {20 User}
 addUserKnob {7 a t "the magnitude (length) of the vector between the centerPoints A and B" R 0 4}
 a {{"sqrt(pow(abs(parent.Transform1.center.x - parent.Transform2.center.x), 2) + pow(abs(parent.Transform1.center.y - parent.Transform1.center.y), 2))"}}
 addUserKnob {7 b t "the magnitude (length) of the vector between the centerPoints B and C" R 0 4}
 b {{"sqrt(pow(abs(parent.Transform2.center.x - parent.Transform3.center.x), 2) + pow(abs(parent.Transform2.center.y - parent.Transform3.center.y), 2))"}}
 addUserKnob {7 c t "the magnitude (length) of the vector between the centerPoints H and A" R 0 4}
 c {{"sqrt(pow(abs(parent.Transform4.center.x - parent.Transform1.center.x), 2) + pow(abs(parent.Transform4.center.y - parent.Transform1.center.y), 2))"}}
 addUserKnob {26 "" +STARTLINE}
 addUserKnob {7 x t "(a*a - b*b + c*c) / 2 * c" R 0 4}
 x {{"(pow(this.a, 2) - pow(this.b, 2) + pow(this.c, 2)) / (2 * c)"}}
 addUserKnob {7 y t "c - x" R 0 4}
 y {{"this.c - this.x"}}
 addUserKnob {7 alpha t "acos(clamp(y/b, -1, 1))" R 0 4}
 alpha {{"acos(clamp(y / b, -1, 1))"}}
 addUserKnob {7 beta t "acos(clamp(x/a, -1, 1))" R 0 4}
 beta {{"acos(clamp(this.x / this.a, -1, 1))"}}
 addUserKnob {12 D t "the vector that connects the centerPoints H (source) and A (destination)"}
 D {{"parent.Transform4.center.x - parent.Transform1.center.x"} {"parent.Transform4.center.y - parent.Transform1.center.y"}}
 addUserKnob {7 delta t "atan2(D\[1], D\[0]);" R 0 4}
 delta {{"atan2(this.D.x, this.D.y)"}}
}
